{
  "manifest_version": 2,
  "name": "Rescomp Autologin",
  "version": "0.0.1",
  "description": "Never log into Rescomp manually ever again.",
  "permissions": [
    "https://*/",
    "http://*/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/jquery.min.js", "js/aes.js", "js/json_formatter.js", "js/main.js"]
    }
  ]
}
